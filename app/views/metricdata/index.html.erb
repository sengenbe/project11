<h1>Listing metricdata</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Value</th>
    <th>Adddate</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @metricdata.each do |metricdatum| %>
  <tr>
    <td><%= metricdatum.name %></td>
    <td><%= metricdatum.value %></td>
    <td><%= metricdatum.adddate %></td>
    <td><%= link_to 'Show', metricdatum %></td>
    <td><%= link_to 'Edit', edit_metricdatum_path(metricdatum) %></td>
    <td><%= link_to 'Destroy', metricdatum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

